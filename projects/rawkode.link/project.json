{
  "name": "rawkode-link",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.link",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.link/src",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run start",
        "cwd": "projects/rawkode.link"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy",
        "cwd": "projects/rawkode.link"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "deploy:direct": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run deploy",
        "cwd": "projects/rawkode.link"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "manage:secrets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just secrets",
        "cwd": "projects/rawkode.link"
      }
    }
  }
}
