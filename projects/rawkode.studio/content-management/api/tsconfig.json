{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "nodenext",
        "outDir": "dist",
        "skipLibCheck": true,
        "strict": true,
        "target": "ESNext",
        "types": ["bun-types"]
    }
}
