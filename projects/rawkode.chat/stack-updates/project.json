{
  "name": "rawkode-chat-stack-updates",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.chat/stack-updates",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.chat/stack-updates",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno task dev",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run main.ts",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno test",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deployctl deploy --project=chat-stack-updates main.ts",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "cache": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno cache main.ts feed.ts store.ts",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno lint",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno fmt",
        "cwd": "projects/rawkode.chat/stack-updates"
      }
    }
  }
}
