{
  "name": "rawkode-chat-standup-bot",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.chat/standup-bot",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.chat/standup-bot/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wrangler dev",
        "cwd": "projects/rawkode.chat/standup-bot"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wrangler deploy",
        "cwd": "projects/rawkode.chat/standup-bot"
      },
      "outputs": ["{projectRoot}/dist"]
    }
  }
}
