# Cloudflare Account ID
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Analytics API Key (generate a secure key)
ANALYTICS_API_KEY=your_secure_api_key_here

# Environment
ENVIRONMENT=development

# Project settings
PROJECT_ID=rawkode.tools

# Buffer settings (optional, defaults in wrangler.toml)
# BUFFER_SIZE=1000
# BUFFER_TIMEOUT_MS=60000

# Iceberg configuration (optional, defaults in wrangler.toml)
# USE_ICEBERG=true
# R2_CATALOG_ENDPOINT=https://your-r2-bucket.r2.cloudflarestorage.com
# COMPACTION_THRESHOLD=50
# TARGET_FILE_SIZE_MB=128