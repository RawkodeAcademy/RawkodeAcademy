---
interface Props {
	content: any;
}

const { content: Content } = Astro.props;

// Override components to be RSS-safe
const components = {
	// Replace ZoomableImage with standard Image component - return null for RSS
	ZoomableImage: () => null,

	// Replace Aside with a simple div - return null for RSS
	Aside: () => null,
};
---

<Content components={components} />