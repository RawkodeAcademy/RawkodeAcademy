{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "design",
	"compatibility_date": "2025-05-05",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./storybook-static/_worker.js/index.js",
	"assets": {
		"binding": "ASSETS",
		"directory": "./storybook-static"
	},
	"observability": {
		"enabled": true
	},
	"routes": [
		{
			"pattern": "design.rawkode.academy",
			"custom_domain": true
		}
	]
}