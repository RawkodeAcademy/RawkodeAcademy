{
  "name": "rawkode-academy-website",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.academy/website",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.academy/website/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dagger call build --source .",
        "cwd": "projects/rawkode.academy/website"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dagger call build-storybook --source .",
        "cwd": "projects/rawkode.academy/website"
      },
      "outputs": ["{projectRoot}/storybook-static"]
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy",
        "cwd": "projects/rawkode.academy/website"
      },
      "dependsOn": ["build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "dev:wrangler": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev:wrangler",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run storybook",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test:watch",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "test:seo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test:seo",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run codegen",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "validate:seo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run validate:seo",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just format",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "invalidate-videos-cache": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just invalidate-videos-cache",
        "cwd": "projects/rawkode.academy/website"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run preview",
        "cwd": "projects/rawkode.academy/website"
      },
      "dependsOn": ["build"]
    }
  }
}
