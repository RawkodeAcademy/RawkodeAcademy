{
  "name": "rawkode-academy-platform-chapters",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.academy/platform/chapters",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.academy/platform/chapters",
  "targets": {
    "start:read-model": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-net --allow-env read-model/main.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "publish:read-model": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-net --allow-env read-model/publish.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "db:migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-net --allow-env data-model/migrate.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "db:seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-net --allow-env data-model/seed.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "import:r2": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-all import-from-r2.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "cache": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno cache read-model/main.ts data-model/migrate.ts data-model/seed.ts import-from-r2.ts read-model/publish.ts",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno lint",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno fmt",
        "cwd": "projects/rawkode.academy/platform/chapters"
      }
    }
  }
}
