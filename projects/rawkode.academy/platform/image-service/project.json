    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run start",
        "cwd": "projects/rawkode.academy/platform/image-service"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test",
        "cwd": "projects/rawkode.academy/platform/image-service"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run format",
        "cwd": "projects/rawkode.academy/platform/image-service"
      }
    },
    "copy-wasm": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just copy-wasm",
        "cwd": "projects/rawkode.academy/platform/image-service"
      }
    }
  }
}
