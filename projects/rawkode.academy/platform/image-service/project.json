{
  "name": "rawkode-academy-platform-image-service",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.academy/platform/image-service",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.academy/platform/image-service/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "projects/rawkode.academy/platform/image-service"
      },
      "outputs": ["{projectRoot}/dist"]
