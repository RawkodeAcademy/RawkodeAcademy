{
  "name": "rawkode-academy-platform-rpc",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.academy/platform/rpc",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.academy/platform/rpc/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "projects/rawkode.academy/platform/rpc"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run preview",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "test:webhooks": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just test-webhooks",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "deploy:astro": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy-astro",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "deploy:trigger": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy-trigger",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    },
    "deploy:restate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "just deploy-restate",
        "cwd": "projects/rawkode.academy/platform/rpc"
      }
    }
  }
}
