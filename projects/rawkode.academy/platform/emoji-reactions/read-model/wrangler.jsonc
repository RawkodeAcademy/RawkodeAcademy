{
	"$schema": "https://unpkg.com/wrangler/config-schema.json",
	"name": "emoji-reactions-read-model",
	"main": "./main.ts",
	"compatibility_date": "2025-04-05",
	"keep_vars": false,
	"minify": true,
	"placement": {
		"mode": "smart"
	},
	"workers_dev": true,
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "emoji-reactions-db",
			"database_id": "your-database-id"
		}
	]
}