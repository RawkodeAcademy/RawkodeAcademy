{
  "name": "rawkode-academy-platform-youtube-to-structured-json",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "root": "projects/rawkode.academy/platform/youtube-to-structured-json",
  "projectType": "application",
  "sourceRoot": "projects/rawkode.academy/platform/youtube-to-structured-json",
  "targets": {
    "download-transcripts": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-all download-transcripts.ts",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "extract-data": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-all extract-data-from-json.ts",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "pipeline": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-all pipeline.ts",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "split-csv": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno run --allow-all split-csv.ts",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "cache": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno cache download-transcripts.ts extract-data-from-json.ts pipeline.ts split-csv.ts",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno lint",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "deno fmt",
        "cwd": "projects/rawkode.academy/platform/youtube-to-structured-json"
      }
    }
  }
}
