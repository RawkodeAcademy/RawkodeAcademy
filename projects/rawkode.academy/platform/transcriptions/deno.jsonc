{
	"nodeModulesDir": "auto",
	"imports": {
		"@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.750.0",
		"@deepgram/sdk": "npm:@deepgram/sdk@^3.11.1",
		"@restatedev/restate-sdk": "npm:@restatedev/restate-sdk@^1.4.0",
		"@sentry/deno": "npm:@sentry/deno@^8.47.0",
		"@sentry/tracing": "npm:@sentry/tracing@^7.114.0",
		"@std/assert": "jsr:@std/assert@1",
		"@std/fs": "jsr:@std/fs@^1.0.5",
		"@std/path": "jsr:@std/path@^1.0.6"
	},
	"compilerOptions": {
		"verbatimModuleSyntax": true,
		"strict": true
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 80,
		"indentWidth": 2,
		"semiColons": true,
		"singleQuote": true,
		"proseWrap": "preserve",
		"include": ["**/**"]
	}
}
