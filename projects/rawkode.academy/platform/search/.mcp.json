{
  "mcpServers": {
    "container-use": {
      "type": "stdio",
      "command": "container-use",
      "args": [
        "stdio"
      ],
      "env": {}
    },
    "shuttle": {
      "type": "stdio",
      "command": "shuttle",
      "args": [
        "mcp",
        "start"
      ],
      "env": {}
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    }
  }
}